# If a config.py file exists, this file is ignored unless it's explicitly loaded
# via config.load_autoconfig(). For more information, see:
# https://github.com/qutebrowser/qutebrowser/blob/main/doc/help/configuring.asciidoc#loading-autoconfigyml
# DO NOT edit this file by hand, qutebrowser will overwrite it.
# Instead, create a config.py - see :help for details.

config_version: 2
settings:
  auto_save.session:
    global: false
  colors.contextmenu.menu.bg:
    global: '#141414'
  colors.contextmenu.menu.fg:
    global: white
  colors.contextmenu.selected.bg:
    global: '#252727'
  colors.contextmenu.selected.fg:
    global: null
  colors.downloads.bar.bg:
    global: '#0f0f0f'
  colors.tabs.bar.bg:
    global: '#00000000'
  colors.tabs.even.bg:
    global: darkgrey
  colors.tabs.selected.even.bg:
    global: '#00000000'
  colors.tabs.selected.even.fg:
    global: white
  colors.webpage.darkmode.algorithm:
    global: lightness-hsl
  colors.webpage.preferred_color_scheme:
    global: dark
  content.blocking.hosts.lists:
    global:
    - https://raw.githubusercontent.com/StevenBlack/hosts/master/hosts
  content.headers.user_agent:
    global: Mozilla/5.0 ({os_info}) AppleWebKit/{webkit_version} (KHTML, like Gecko)
      {upstream_browser_key}/{upstream_browser_version_short} Safari/{webkit_version}
  content.javascript.clipboard:
    https://github.com: access-paste
  downloads.position:
    global: bottom
  fonts.web.family.fixed:
    global: poppins
  fonts.web.family.sans_serif:
    global: poppins
  fonts.web.family.serif:
    global: poppins
  fonts.web.family.standard:
    global: poppins
  scrolling.smooth:
    global: true
  tabs.show:
    global: always
  tabs.width:
    global: 10%
  url.default_page:
    global: file:///home/cr1ogen/.config/qutebrowser/Startpages/sijan/index.html
  url.start_pages:
    global: file:///home/cr1ogen/.config/qutebrowser/Startpages/sijan/index.html
  window.transparent:
    global: true
